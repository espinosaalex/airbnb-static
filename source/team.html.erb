<html>
  <head>
    <meta charset="utf-8">
    <title>Airbnb team</title>
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,700|Raleway:300,400,500,700">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="css/style.css">
  </head>

  <body>
    <div class="container text-center padded">
      <h1>Our team</h1>
      <ul class="list-inline text-center">
        <% ["alex", "julia", "drew"].each do |member| %>
          <li>
            <h2><%= member.capitalize %></h2>
            <img src="https://placehold.it/100x100" alt="" class="img-circle">
          </li>
        <% end %>
      </ul>

      <h3 class="padded">
        <% link_to "mailto:contact@airbnb.com" do %>
          <i class="fa fa-envelope"></i> contact@airbnb.com
        <% end %>
        </a>
      </h3>
    </div>
  </body>
</html>
